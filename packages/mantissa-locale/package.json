{
  "name": "@mantissa/locale",
  "version": "1.0.0",
  "private": true,
  "description": "@mantissa/locale",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Boris Ryabov",
    "email": "rv.boris@gmail.com"
  },
  "keywords": [
    "locale"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/Locale.js",
  "typings": "lib/Locale.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "accept-language": "^3.0.18"
  },
  "devDependencies": {
    "typescript": "^3.4.5",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "jest": "^24.7.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
